{
    "$schema": "https://schema.tauri.app/config/2",
    "productName": "PromptVault",
    "version": "0.1.0",
    "identifier": "com.promptvault.app",
    "build": {
      "beforeDevCommand": "npm run dev",
      "devUrl": "http://localhost:1420",
      "beforeBuildCommand": "npm run build",
      "frontendDist": "../dist"
    },
    "app": {
      "withGlobalTauri": true,
      "windows": [
        {
          "title": "PromptVault",
          "width": 1200,
          "height": 800,
          "resizable": true,
          "fullscreen": false,
          "center": true
        }
      ],
      "security": {
        "csp": null
      }
    },
    "bundle": {
      "active": false
    }
  }
  